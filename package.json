{
  "name": "react-redux-starter",
  "version": "0.0.0",
  "description": "A boilerplate to help starting react-redux projects",
  "main": "src/js/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel/register --recursive --reporter nyan || true",
    "cover": "babel-node node_modules/.bin/isparta cover node_modules/.bin/_mocha -- --recursive --reporter nyan || true",
    "watch": "webpack-dev-server --inline --hot || true",
    "build": "webpack || true",
    "docs": "esdoc -c esdoc.json || true"
  },
  "author": "Dan Coates",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.19.0",
    "esdoc": "^0.3.0",
    "eslint": "^1.5.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.4.2",
    "expect": "^1.10.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "isparta": "^3.0.4",
    "mocha": "^2.3.3",
    "node-sass": "^3.3.3",
    "postcss-loader": "^0.6.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.2",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.11.0"
  },
  "dependencies": {
    "react": "^0.13.3",
    "react-redux": "^2.1.2",
    "redux": "^3.0.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.3.3"
  }
}

{
  "name": "react-redux-starter",
  "version": "0.0.0",
  "description": "A boilerplate to help starting react-redux projects",
  "main": "src/js/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel/register --recursive --reporter nyan || true",
    "cover": "babel-node node_modules/.bin/isparta cover --report text --report html node_modules/.bin/_mocha -- --recursive --reporter nyan || true",
    "watch": "node runner watch || true",
    "build": "node runner build || true",
    "docs": "esdoc -c esdoc.json "
  },
  "author": "Dan Coates",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-loader": "^5.3.2",
    "esdoc": "^0.3.0",
    "eslint": "^1.5.0",
    "eslint-loader": "^1.0.0",
    "isparta": "^3.0.4",
    "mocha": "^2.3.3",
    "node-sass": "^3.3.3",
    "redux-devtools": "^2.1.2",
    "sass-loader": "^2.0.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.11.0"
  },
  "dependencies": {
    "react": "^0.13.3",
    "react-redux": "^2.1.2",
    "redux": "^3.0.0"
  }
}
